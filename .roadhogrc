{
  "entry": "src/pages/*.js",
  "multipage": true,
  "disableCSSModules": false,
  "publicPath": "/",
  "autoprefixer": {
    "browsers": [
      "iOS >= 8", "Android >= 4"
    ]
  },
  "proxy": {
    "/": {
      "target": "http://localhost:5000",
      "changeOrigin": true,
      "pathRewrite": { "^/" : "" }
    }
  },
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  }
}
